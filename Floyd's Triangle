import java.util.*;
import java.io.*;
class main{
    public static void main (String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter number of rows");
        int n=sc.nextInt();
        int j=1;
        for(int i=1;i<=n;i++){
            int c=i;
            while(c>0){
                System.out.print(j+" ");
                j++;
                c--;
            }
            System.out.println();
        }
        
    }
}
